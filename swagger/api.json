          "favorite",
          "name",
          "phone"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "name": {
            "type": "string"
          },
          "email": {
            "type": "string",
            "format": "email"
          },
          "phone": {
            "type": "string"
          },
          "favorite": {
            "type": "boolean"
          },
          "owner": {
            "type": "integer"
          },
          "createdAt": {
            "type": "string",
            "format": "2023-12-18T20:17:12.878+00:00"
          },
          "updatedAt": {
            "type": "string",
            "format": "2023-12-18T20:17:12.878+00:00"
          }
        }
      },
      "Contacts": {
        "type": "array",
        "items": {
          "$ref": "#/components/schemas/Contact"
        }
      },
      "Error": {
        "required": [
          "code",
          "message"
        ],
        "type": "object",
        "properties": {
          "code": {
            "type": "integer"
          },
          "message": {
            "type": "string"
          }
        }
      },
      "Favorite": {
        "required": [
          "favorite"
        ],
        "type": "object",
        "properties": {
          "favorite": {
            "type": "boolean"
          }
        }
      },
      "UserRegistrationRes": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "format": "email"
          },
          "subscription": {
            "type": "string"
          },
          "createdAt": {
            "type": "string",
            "format": "2023-12-18T20:17:12.878+00:00"
          },
