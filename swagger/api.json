        "required": [
          "email",
          "password"
        ],
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "format": "email"
          },
          "password": {
            "maxLength": 15,
            "minLength": 5,
            "type": "string"
          }
        }
      },
      "UserLoginRes": {
        "type": "object",
        "properties": {
          "token": {
            "type": "string"
          },
          "user": {
            "$ref": "#/components/schemas/UserLoginRes_user"
          }
        }
      },
